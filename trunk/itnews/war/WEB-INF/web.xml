<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">


	<servlet>
		<servlet-name>Itnews</servlet-name>
		<servlet-class>com.s7soft.gae.itnews.ItnewsServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CornKoServlet</servlet-name>
		<servlet-class>com.s7soft.gae.itnews.CornKoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CornJaServlet</servlet-name>
		<servlet-class>com.s7soft.gae.itnews.CornJaServlet</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>Itnews</servlet-name>
		<url-pattern>/itnews</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CornKoServlet</servlet-name>
		<url-pattern>/cronko</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CornJaServlet</servlet-name>
		<url-pattern>/cronja</url-pattern>
	</servlet-mapping>



	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
